{
  "expo": {
    "name": "COVID-19 tracker",
    "slug": "corona-virus",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.chuks.covidtracker",
      "versionCode": 1
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "COVID-19 Tracker",
  "name": "covid_tracker"
}